#include<iostream>
#include<iomanip>
using namespace std;

int main(){

    float SGPA,CGPA;

    cin>>SGPA>>CGPA;
    

    cout<<"Student ID"<<setw(4)<<":"<<setw(1)<<"24CE122"<<endl;
    cout<<"Student Name"<<setw(2)<<":"<<setw(1)<<"Fayan Suthar"<<endl<<endl;
    cout<<setw(25)<<"TH"<<setw(3)<<"PR"<<endl;
    cout<<"Computer Programming"<<setw(5)<<"B+"<<setw(3)<<"A "<<endl;
    cout<<"Math"<<setw(21)<<"B+"<<setw(3)<<"O"<<endl;
  

    cout<<endl<<endl<<"SGPA"<<setw(1)<<":"<<setw(1)<<"SGPA"<<endl;
    cout<<"CGPA"<<setw(1)<<":"<<setw(1)<<"CGPA"<<endl;


    cout<<"SGPA"<<" |"<<"CGPA"<<endl;
    cout<<left<<setw(5)<<SGPA;
    cout<<right<<setw(5)<<CGPA;

}
